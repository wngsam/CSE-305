<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
    <class name="BlindDate" table="blinddate">
        <id name="profileA" column="PROFILEA"></id>
        <property name="profileB" column="PROFILEB" />
        <property name="profileC" column="PROFILEC" />
        <property name="dateTime" column="DATE_TIME" />
    </class> 
</hibernate-mapping>