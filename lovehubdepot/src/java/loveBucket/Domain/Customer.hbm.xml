<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
    <class name="Customer" table="user">
        <id name="SSN" column="SSN"></id>
        <property name="PPP" column="PPP" />
        <property name="rating" column="RATING" />
        <property name="dateOfLastAct" column="DATEOFLASTACT" />
    </class> 
</hibernate-mapping>