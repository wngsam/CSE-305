<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
    <class name="Account" table="account">
        <id name="ownerSSN" column="OWNERSSN"></id>
        <property name="cardNumber" column="CARDNUMBER" />
        <property name="acctNum" column="ACCTNUM" />
        <property name="acctCreationDate" column="ACCTCREATIONDATE" />
    </class> 
</hibernate-mapping>