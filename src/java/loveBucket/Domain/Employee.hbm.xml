<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
  <class name="loveBucket.Domain.Employee" table="employee">
    <id column="SSN" name="SSN"/>
    <property column="ROLE" name="role"/>
    <property column="STARTDATE" name="startDate"/>
    <property column="HOURLYRATE" name="hourlyRate"/>
  </class>
</hibernate-mapping>
