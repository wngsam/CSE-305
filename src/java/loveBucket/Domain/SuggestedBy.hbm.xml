<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
    <class name="SuggestedBy" table="suggestedby">
        <id name="custRep" column="PROFILEID"></id>
        <property name="profile1" column="PROFILE1" />
        <property name="profile2" column="PROFILE2" />
