<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="loveBucket.Domain">
  <class name="loveBucket.Domain.User" table="user">
    <id column="SSN" name="SSN"/>
    <property column="PPP" name="PPP"/>
    <property column="Rating" name="Rating"/>
    <property column="DateOfLastAct" name="DateOfLastAct"/>
  </class>
</hibernate-mapping>
